{
  "expo": {
    "name": "EverStock",
    "slug": "EverStock_UI",
    "backgroundColor": "#1C1A1A",
    "scheme": "your-app-scheme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1C1A1A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vadimbr.EverStock"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1C1A1A"
      },
      "navigationBar": {
        "backgroundColor": "#242121",
        "barStyle": "light-content"
      },
      "package": "com.vadimbr.EverStock_UI"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "adaf8507-159b-4b9a-a307-842981bc62e5"
      }
    }
  }
}
